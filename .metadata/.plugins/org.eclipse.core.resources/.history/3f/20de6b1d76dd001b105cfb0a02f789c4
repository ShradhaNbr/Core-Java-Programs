import java.util.Scanner;

public class Power {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter Power Value Between 0 to 31");
		int n = sc.nextInt();
		sc.close();

		for (int i = 1; i <= n; i++) {
			int power = (int) Math.pow(2, i);
			System.out.print(power + " ");
		}
		
	}

}